<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context="com.example.seymur.azercell2.fragments.Ayar">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:weightSum="1">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:layout_editor_absoluteY="0dp"
            tools:layout_editor_absoluteX="0dp">

            <ImageButton
                android:id="@+id/mapBtn"
                app:srcCompat="@drawable/maps"
                android:layout_width="125dp"
                android:layout_height="125dp"
                android:layout_weight="0.04"
                android:background="@drawable/round_button"
                android:layout_marginRight="32dp"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginLeft="0dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="32dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="8dp"
                app:layout_constraintHorizontal_bias="0.17"
                app:layout_constraintVertical_bias="0.626" />

            <ImageButton
                android:id="@+id/onlineServiceBtn"
                android:layout_width="125dp"
                android:layout_height="125dp"
                android:layout_weight="0.04"
                android:background="@drawable/round_button"
                app:srcCompat="@drawable/onlineserv"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="24dp"
                app:layout_constraintVertical_chainStyle="spread"
                android:layout_marginRight="32dp"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginLeft="0dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="32dp"
                app:layout_constraintHorizontal_bias="0.14"
                app:layout_constraintVertical_bias="0.194" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginTop="8dp"
                android:text="@string/onlineservices"
                android:textAlignment="center"
                app:layout_constraintHorizontal_bias="0.509"
                app:layout_constraintLeft_toLeftOf="@+id/onlineServiceBtn"
                app:layout_constraintRight_toRightOf="@+id/onlineServiceBtn"
                app:layout_constraintTop_toBottomOf="@+id/onlineServiceBtn" />

            <ImageButton
                android:id="@+id/simsimBtn"
                android:layout_width="125dp"
                android:layout_height="125dp"
                android:layout_weight="1"
                android:background="@drawable/round_button"
                app:srcCompat="@drawable/simsim"
                tools:ignore="ContentDescription"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="24dp"
                android:layout_marginRight="0dp"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginLeft="32dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="32dp"
                app:layout_constraintHorizontal_bias="0.975"
                app:layout_constraintVertical_bias="0.194" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginTop="8dp"
                android:text="@string/simsim"
                android:textAlignment="center"
                app:layout_constraintHorizontal_bias="0.45"
                app:layout_constraintLeft_toLeftOf="@+id/simsimBtn"
                app:layout_constraintRight_toRightOf="@+id/simsimBtn"
                app:layout_constraintTop_toBottomOf="@+id/simsimBtn" />

            <ImageButton
                android:id="@+id/trafficUsage"
                android:layout_width="125dp"
                android:layout_height="125dp"
                android:layout_marginBottom="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginStart="8dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/round_button"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.91"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.649"
                app:srcCompat="@drawable/speedtest" />

            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:layout_marginTop="8dp"
                android:text="@string/trafficUsage"
                android:textAlignment="center"
                app:layout_constraintHorizontal_bias="0.553"
                app:layout_constraintLeft_toLeftOf="@+id/trafficUsage"
                app:layout_constraintRight_toRightOf="@+id/trafficUsage"
                app:layout_constraintTop_toBottomOf="@+id/trafficUsage" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="8dp"
                android:text="@string/nearestoficces"
                android:textAlignment="center"
                app:layout_constraintLeft_toLeftOf="@+id/mapBtn"
                app:layout_constraintRight_toRightOf="@+id/mapBtn"
                app:layout_constraintHorizontal_bias="0.25"
                android:layout_marginTop="8dp"
                app:layout_constraintTop_toBottomOf="@+id/mapBtn" />

        </android.support.constraint.ConstraintLayout>

    </LinearLayout>

</FrameLayout>
