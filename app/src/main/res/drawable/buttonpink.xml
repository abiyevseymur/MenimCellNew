<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >

    <item  android:state_pressed="false">
        <shape android:shape="rectangle"  >
            <corners android:radius="10dp" />
            <stroke android:width="1dp" android:color="@color/pink" />
            <gradient android:angle="0" android:startColor="@color/pink" android:endColor="@color/pink" />
        </shape>
    </item>
    <item  android:state_pressed="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="10dp" />
            <stroke android:width="1dp" android:color="@color/pinkAcces" />
            <gradient android:angle="0" android:startColor="@color/pinkAcces" android:endColor="@color/pinkAcces" />
        </shape>
    </item>
</selector>