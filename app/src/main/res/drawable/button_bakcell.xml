<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:state_pressed="false">
        <shape android:shape="rectangle"  >
            <corners android:radius="10dp" />
            <stroke android:width="1dp" android:color="@color/colorBakcellBtn" />
            <gradient android:angle="0" android:startColor="@color/colorBakcellBtn" android:endColor="@color/colorBakcellBtn" />
        </shape>
    </item>
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="10dp" />
            <stroke android:width="1dp" android:color="@color/colorBakcellTab" />
            <gradient android:angle="0" android:startColor="@color/colorBakcellTab" android:endColor="@color/colorBakcellTab" />
        </shape>
    </item>
</selector>